<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.meng.library.mybatisplus.mapper.UserHeadUrlMapper">

    <select id="pageUserHeadUrlJoin" resultType="com.meng.library.mybatisplus.result.UserHeadUrlResult">
        SELECT
             a.id,
             a.account,
             a.password,
             a.state,
             b.user_head_img,
             b.create_time,
             b.update_time
        FROM user a
        LEFT JOIN user_head_url b ON a.id = b.user_id
        ${ew.customSqlSegment}
    </select>
</mapper>
